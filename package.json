{
  "name": "mb-shop",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Maridalen Brenneri Backoffice",
  "engines": {
    "node": "12.2.0"
  },
  "main": "./server/dist/server.js",
  "scripts": {
    "install-client": "npm install --prefix ./client/",
    "install-server": "npm install --prefix ./server/",
    "postinstall-client": "npm run postinstall --prefix ./client/",
    "postinstall-server": "npm run postinstall --prefix ./server/",
    "postinstall": "npm-run-all install-client install-server postinstall-client postinstall-server",
    "build-client": "npm run build --prefix ./client/",
    "build-server": "npm run build --prefix ./server/",
    "build": "npm-run-all build-client build-server",
    "dev": "npm run dev --prefix ./server/",
    "start": "node ./server/dist/server.js"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
