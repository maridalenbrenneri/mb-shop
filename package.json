{
  "name": "mb-shop-api",
  "version": "1.0.1",
  "description": "Maridalen Brenneri Backoffice",
  "engines": {
    "node": "11.2.0"
  },
  "main": "./server/dist/server.js",
  "scripts": {
    "postinstall-client": "ng build --aot --prod --prefix ./client/",
    "postinstall-server": "npm run build --prefix ./server/",
    "postinstall": "npm-run-all postinstall-client postinstall-server",
    "build-client": "npm run build --prefix ./client/",
    "build-server": "npm run build --prefix ./server/",
    "build": "npm-run-all build-client build-server",
    "dev": "npm run dev --prefix ./server/",
    "start": "nodemon .server/dist/server.js"
  },
  "dependencies": {
    "@types/node": "^10.5.6",
    "typescript": "^3.0.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "ts-node": "^7.0.1"
  }
}