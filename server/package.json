{
  "name": "mb-shop-api",
  "version": "1.0.0",
  "description": "Maridalen Brenneri Api",
  "license": "ISC",
  "engines": {
    "node": "10.15.3"
  },
  "main": "./dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "postinstall": "npm run build",
    "start": "nodemon ./dist/server.js",
    "test": "mocha -r ts-node/register test/**/*test.ts",
    "database": "ts-node ./src/database/index.ts",
    "add-delivery-days": "ts-node ./src/jobs/add-delivery-days.ts",
    "add-business-subscriptions": "ts-node ./src/jobs/add-business-subscriptions.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "./dist",
      "client"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "dependencies": {
    "@types/node": "10.5.6",
    "axios": "0.19.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "dotenv": "6.2.0",
    "email-validator": "2.0.4",
    "express": "4.16.4",
    "fast-xml-parser": "3.16.0",
    "foreman": "3.0.1",
    "jsonwebtoken": "8.3.0",
    "moment": "2.25.0",
    "mysql2": "1.6.4",
    "pg": "7.4.3",
    "request": "2.88.0",
    "sequelize": "5.15.1",
    "typescript": "3.0.1",
    "winston": "3.0.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "nodemon": "1.19.4",
    "ts-node": "7.0.1"
  }
}
