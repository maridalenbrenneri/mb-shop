{
  "name": "mb-shop-api",
  "version": "1.0.0",
  "description": "Maridalen Brenneri Api",
  "license": "ISC",
  "engines": {
    "node": "10.15.3"
  },
  "main": "./dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "postinstall": "npm run build",
    "start": "nodemon ./dist/server.js",
    "database": "ts-node ./src/database/index.ts",
    "add-delivery-days": "ts-node ./src/jobs/add-delivery-days.ts",
    "add-business-subscriptions": "ts-node ./src/jobs/add-business-subscriptions.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "./dist",
      "client"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "dependencies": {
    "@types/node": "13.13.4",
    "axios": "0.19.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "email-validator": "2.0.4",
    "express": "4.17.1",
    "fast-xml-parser": "3.16.0",
    "foreman": "3.0.1",
    "jsonwebtoken": "8.5.1",
    "moment": "^2.24.0",
    "pg": "8.0.3",
    "request": "2.88.2",
    "sequelize": "5.21.7",
    "typescript": "3.8.3",
    "winston": "3.2.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "nodemon": "2.0.3",
    "ts-node": "8.9.1"
  }
}
